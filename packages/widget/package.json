{
  "name": "@fidbak/widget",
  "version": "0.1.13",
  "description": "Framework-free feedback FAB + modal (UMD for CDN, ESM for npm)",
  "type": "module",
  "main": "dist/fidbak.fab.min.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./umd": {
      "default": "./dist/fidbak.fab.min.global.js"
    }
  },
  "unpkg": "dist/fidbak.min.global.js",
  "jsdelivr": "dist/fidbak.min.global.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "feedback",
    "widget",
    "umd",
    "esm",
    "cloudflare",
    "workers"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenny-io/fidbak.git"
  },
  "bugs": {
    "url": "https://github.com/kenny-io/fidbak/issues"
  },
  "homepage": "https://fidbak-dash.pages.dev/",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
